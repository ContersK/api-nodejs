{
  "dependencies": {
    "@prisma/client": "^6.4.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "prisma": "^6.4.0",
    "sqlite3": "^5.1.7",
    "yup": "^1.6.1"
  },
  "name": "api-rest-node-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "tsc",
    "build": "tsc",
    "production": "node build/index.js",
    "dev": "ts-node-dev ./src/index.ts",
    "start": "ts-node-dev ./src/index.ts",
    "knex:migrate": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:latest",
    "knex:rollback-all": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:rollback --all",
    "knex:rollback": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:rollback",
    "knex:seed": "knex --knexfile ./src/server/database/knex/Enviroment.ts seed:run",
    "test": "jest"
  },
  "author": "Conters",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
